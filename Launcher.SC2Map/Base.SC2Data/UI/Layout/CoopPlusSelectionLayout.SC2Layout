<?xml version="1.0" encoding="utf-8" standalone="yes"?>
<Desc>
    <Frame type="Frame" name="IntentionArrowImageTemplate">
        <Frame type="Image" name="Arrow">
            <Anchor relative="$parent"/>
            <Rotation val="180"/>
            <Alpha val="100"/>
            <Texture val="Assets/Textures/sc2_ui_ingame_replay_effect_arrow.dds"/>
        </Frame>

        <Frame type="Image" name="Arrow_Highlight">
            <Anchor relative="$parent"/>
            <Rotation val="{$parent/Arrow/@Rotation}"/>
            <Texture val="Assets/Textures/sc2_ui_ingame_replay_effect_arrow_highlight.dds"/>

            <Animation name="StaticAnim">
                <Event event="OnShown" frame="$parent" action="Reset,Play"/>

                <Controller type="Fade" end="Loop">
                    <Key type="Curve" time="0" value="0" out="Fast"/>
                    <Key type="Curve" time="1" value="0" in="Slow"/>
                    <Key type="Curve" time="1.1" value="70" in="Slow"/>
                    <Key type="Curve" time="1.2" value="0" out="Fast"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>

    <Frame type="Frame" name="CommanderSelectionArrowImageTopTemplate">
        <Frame type="Image" name="Arrow1">
            <Anchor relative="$parent"/>

            <TextureCoordRotation val="90"/>
            <Alpha val="180"/>
            <Texture val="Assets/Textures/sc2_ui_ingame_replay_effect_arrow.dds"/>
            <Color val="255,255,255,0"/>
        </Frame>

        <Frame type="Image" name="Arrow1_Highlight">
            <Anchor relative="$parent/Arrow1"/>

            <TextureCoordRotation val="{$parent/Arrow1/@TextureCoordRotation}"/>
            <Texture val="Assets/Textures/sc2_ui_ingame_replay_effect_arrow_highlight.dds"/>
            <Color val="{$parent/Arrow1/@Color}"/>

            <Animation name="StaticAnim">
                <Event event="OnShown" frame="$parent" action="Reset,Play"/>

                <Controller type="Fade" end="Loop">
                    <Key type="Curve" time="0" value="120"/>
                    <Key type="Curve" time="0.8" value="120"/>
                    <Key type="Curve" time="0.9" value="0"/>
                    <Key type="Curve" time="1" value="120"/>
                </Controller>
            </Animation>
        </Frame>
    </Frame>

    <Frame type="Frame" name="CommanderSelectionArrowImageBottomTemplate" template="CoopPlusSelectionLayout/CommanderSelectionArrowImageTopTemplate">
        <Frame type="Image" name="Arrow1">
            <Anchor relative="$parent"/>

            <TextureCoordRotation val="270"/>
            <Color val="255,255,0,255"/>
        </Frame>
    </Frame>

    <Frame type="Button" name="AlternateButtonTemplate">
        <DisplayType val="ImageLayer"/>
        <ClickSound val="@UI_ActionButtonSelect"/>
        <Style val="@StandardButtonAlternate"/>
        <Width val="288"/>
        <Height val="76"/>
        <HoverSound val="@UI_HoverTick"/>
                
        <Frame type="Image" name="NormalImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <StateCount val="1"/>
            <LayerCount val="4"/>
            <Texture val="@UI/Glues_Battlenet_AlternateButton_Normal" layer="0"/>
            <Texture val="@UI/Glues_Battlenet_AlternateButton_Down" layer="1"/>
            <Texture val="@UI/Glues_Battlenet_AlternateButton_Selected" layer="2"/>
            <Texture val="@UI/Glues_Battlenet_AlternateButton_Disabled" layer="3"/>
            <TextureType val="EndCap" layer="0"/>
            <TextureType val="EndCap" layer="1"/>
            <TextureType val="EndCap" layer="2"/>
            <TextureType val="EndCap" layer="3"/>
            <Tiled val="false"/>
            <DesaturateOnDisable val="false"/>
        </Frame>

        <Frame type="Image" name="HoverImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <StateCount val="1"/>
            <LayerCount val="4"/>
            <Texture val="@UI/Glues_Battlenet_AlternateButton_Hover" layer="0"/>
            <Texture val="@UI/Glues_Battlenet_AlternateButton_Down" layer="1"/>
            <Texture val="@UI/Glues_Battlenet_AlternateButton_Selected" layer="2"/>
            <Texture val="@UI/Glues_Battlenet_AlternateButton_Disabled" layer="3"/>
            <TextureType val="EndCap" layer="0"/>
            <TextureType val="EndCap" layer="1"/>
            <TextureType val="EndCap" layer="2"/>
            <TextureType val="EndCap" layer="3"/>
            <Tiled val="false"/>
            <DesaturateOnDisable val="false"/>
        </Frame>

        <Frame type="Label" name="Label">
            <Anchor side="Top" relative="$parent" pos="Min" offset="3"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="20"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="-20"/>
            <Style val="BattlenetAlternateButton"/>
        </Frame>

        <Frame type="Frame" name="HitTestFrame">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Bottom" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        </Frame>
    </Frame>

    <Frame type="Button" name="CommanderSelectionButtonTemplate">
        <Width val="105"/>
        <Height val="126"/>
        <AllowedButtons val="Left|Right"/>
        
        <Frame type="Image" name="Portrait">
            <Anchor relative="$parent" side="Top" pos="Min" offset="10"/>
            <Anchor relative="$parent" side="Bottom" pos="Max" offset="-10"/>
            <Anchor relative="$parent" side="Left" pos="Min" offset="10"/>
            <Anchor relative="$parent" side="Right" pos="Max" offset="-10"/>
            <DesaturateOnDisable val="false"/>
            <Texture val=""/>
        </Frame>
        
        <Frame type="Image" name="Border">
            <Anchor relative="$parent"/>
            <Texture val="@UI/SC2_UI_Coop_SelectTile_Basic_Selected"/>
            <TextureType val="Nineslice"/>
        </Frame>

        <Frame type="Frame" name="MySelectionArrow" template="CoopPlusSelectionLayout/CommanderSelectionArrowImageTopTemplate">
            <Anchor relative="$parent" side="Top" pos="Min" offset="-25"/>
            <Anchor relative="$parent" side="Bottom" pos="Min" offset="8"/>
            <Anchor relative="$parent" side="Left" pos="Min" offset="20"/>
            <Anchor relative="$parent" side="Right" pos="Max" offset="-20"/>
            <Unclipped val="true"/>
        </Frame>

        <Frame type="Frame" name="TheirSelectionArrow" template="CoopPlusSelectionLayout/CommanderSelectionArrowImageBottomTemplate">
            <Anchor relative="$parent" side="Top" pos="Max" offset="-8"/>
            <Anchor relative="$parent" side="Bottom" pos="Max" offset="25"/>
            <Anchor relative="$parent" side="Left" pos="Min" offset="20"/>
            <Anchor relative="$parent" side="Right" pos="Max" offset="-20"/>
            <Unclipped val="true"/>
        </Frame>

        <Frame type="Image" name="VetoSign">
            <Anchor relative="$parent" side="Top" pos="Mid" offset="-60"/>
            <Anchor relative="$parent" side="Bottom" pos="Mid" offset="60"/>
            <Anchor relative="$parent" side="Left" pos="Mid" offset="-60"/>
            <Anchor relative="$parent" side="Right" pos="Mid" offset="60"/>

            <Texture val="Assets\Textures\btn-command-cancel.dds"/>
            <Color val="255,0,0"/>
        </Frame>        
        
        <StateGroup name="MySelectionState">
            <DefaultState val="Unselected"/>
            
            <State name="Selected">
                <When type="AnimationState" MySelectionState="Selected"/>
                <Action type="SetProperty" frame="MySelectionArrow" visible="true"/>
                <Action type="SetProperty" frame="VetoSign" visible="false"/>
            </State>
            
            <State name="Veto">
                <When type="AnimationState" MySelectionState="Veto"/>
                <Action type="SetProperty" frame="MySelectionArrow" visible="false"/>
                <Action type="SetProperty" frame="VetoSign" visible="true"/>
            </State>
            
            <State name="Unselected">
                <Action type="SetProperty" frame="MySelectionArrow" visible="false"/>
                <Action type="SetProperty" frame="VetoSign" visible="false"/>
            </State>
        </StateGroup>

        <StateGroup name="TheirSelectionState">
            <DefaultState val="Unselected"/>
            
            <State name="Selected">
                <When type="AnimationState" TheirSelectionState="Selected"/>
                <Action type="SetProperty" frame="TheirSelectionArrow" visible="true"/>
            </State>
            
            <State name="Unselected">
                <Action type="SetProperty" frame="TheirSelectionArrow" visible="false"/>
            </State>
        </StateGroup>

        <StateGroup name="PlayerState">
            <DefaultState val="PlayerLhs"/>
            
            <State name="PlayerRhs">
                <When type="AnimationState" PlayerState="PlayerRhs"/>
                <Action type="SetProperty" frame="MySelectionArrow/Arrow1" color="255,255,0,255"/>
                <Action type="SetProperty" frame="TheirSelectionArrow/Arrow1" color="255,255,255,0"/>
            </State>
            
            <State name="PlayerLhs">
                <Action type="SetProperty" frame="MySelectionArrow/Arrow1" color="255,255,255,0"/>
                <Action type="SetProperty" frame="TheirSelectionArrow/Arrow1" color="255,255,0,255"/>
            </State>
        </StateGroup>
    </Frame>

    <Frame type="CutsceneFrame" name="BackgroundCutsceneTemplate">
        <RenderType val="HDR"/>
        <File val="Cutscenes/UI_Screens_AlliedCommander_Background.SC2Cutscene"/>
        
        <Animation name="Show">
            <Event event="OnShown" action="Reset,Play"/>
            
            <Controller type="Cutscene" end="Loop">
                <Key type="Cutscene" action="Play" time="0.0"/>
            </Controller>
        </Animation>
        
        <Animation name="SetStandardBackground">
            <Event event="ModeStandard" action="Reset,Play"/>
            
            <Controller type="Cutscene" end="Pause">
                <Key type="Cutscene" time="0" action="SetCutscene" file="Cutscenes/UI_Screens_AlliedCommander_Background.SC2Cutscene"/>
                <Key type="Cutscene" time="0.0" action="Play"/>
            </Controller>
        </Animation>

        <Animation name="SetBrutalPlusBackground">
            <Event event="ModeBrutalPlus" action="Reset,Play"/>
            
            <Controller type="Cutscene" end="Pause">
                <Key type="Cutscene" time="0" action="SetCutscene" file="Cutscenes/UI_Screens_CollectionsBG.SC2Cutscene"/>
                <Key type="Cutscene" time="0.0" action="Play"/>
            </Controller>
        </Animation>
        
        <Animation name="SetMutatorBackground">
            <Event event="ModeMutator" action="Reset,Play"/>
            
            <Controller type="Cutscene" end="Pause">
                <Key type="Cutscene" time="0" action="SetCutscene" file="Cutscenes/UI_Screens_AlliedCommander_Mutators_Background.SC2Cutscene"/>
                <Key type="Cutscene" time="0.0" action="Play"/>
            </Controller>
        </Animation>
        
        <Animation name="SetCustomBackground">
            <Event event="ModeCustom" action="Reset,Play"/>
            
            <Controller type="Cutscene" end="Pause">
                <Key type="Cutscene" time="0" action="SetCutscene" file="Cutscenes/UI_Screens_AlliedCommander_CustomMutators_Background.SC2Cutscene"/>
                <Key type="Cutscene" time="0.0" action="Play"/>
            </Controller>
        </Animation>
    </Frame>

    <Frame type="CutsceneFrame" name="ForegroundCutsceneTemplate">
        <RenderType val="HDR"/>
        <HasRenderTarget val="true"/>
        
        <Animation name="Show">
            <Event event="OnShown" action="Reset"/>
            <Event event="OnShown" action="Play"/>
            
            <Controller type="Cutscene" end="Loop">
                <Key type="Cutscene" action="Play" time="0.0"/>
            </Controller>
        </Animation>

        <Animation name="CharacterChange">
            <Event event="CharacterChanged" action="Reset"/>
            <Event event="CharacterChanged" action="Play"/>
            
            <Controller type="Cutscene" end="Stop">
                <Key type="Cutscene" action="Bookmark" time="0.0" bookmark="CharacterBirthStart"/>
            </Controller>
        </Animation>

        <Animation name="CharacterReadyStart">
            <Event event="Ready" action="Reset"/>
            <Event event="Ready" action="Play"/>

            <Controller type="Cutscene" end="Stop">
                <Key type="Cutscene" action="Bookmark" time="0.0" bookmark="CharacterReadyStart"/>
            </Controller>
        </Animation>

        <Animation name="CharacterReadyCancel">
            <Event event="NotReady" action="Reset"/>
            <Event event="NotReady" action="Play"/>

            <Controller type="Cutscene" end="Stop">
                <Key type="Cutscene" action="Bookmark" time="0.0" bookmark="CharacterReadyCancel"/>
            </Controller>
        </Animation>
        
        <Animation name="SetStandardLight">
            <Event event="ModeStandard" action="Reset,Play"/>
            
            <Controller type="Cutscene" end="Pause">
                <Key type="Cutscene" action="AddFilter" time="0.0" filter="Standard"/>
                <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Mutator"/>
                <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Custom"/>
            </Controller>
        </Animation>

        <Animation name="SetBrutalPlusLight">
            <Event event="ModeBrutalPlus" action="Reset,Play"/>
            
            <Controller type="Cutscene" end="Pause">
                <Key type="Cutscene" action="AddFilter" time="0.0" filter="Standard"/>
                <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Mutator"/>
                <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Custom"/>
            </Controller>
        </Animation>
        
        <Animation name="SetMutatorLight">
            <Event event="ModeMutator" action="Reset,Play"/>
            
            <Controller type="Cutscene" end="Pause">
                <Key type="Cutscene" action="AddFilter" time="0.0" filter="Mutator"/>
                <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Standard"/>
                <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Custom"/>
            </Controller>
        </Animation>
        
        <Animation name="SetCustomLight">
            <Event event="ModeCustom" action="Reset,Play"/>
            
            <Controller type="Cutscene" end="Pause">
                <Key type="Cutscene" action="AddFilter" time="0.0" filter="CustomMutator"/>
                <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Standard"/>
                <Key type="Cutscene" action="RemoveFilter" time="0.0" filter="Mutator"/>
            </Controller>
        </Animation>
    </Frame>

    <Frame type="Button" name="NavigationButtonTemplate" template="StandardNavigationTemplates/BattlenetTopNavButtonTemplate">
        <ClickSound val="@UI_ScreenNavigation_ButtonClick01"/>
        <HoverSound val="@UI_ScreenNavigation_MouseOver01"/>

        <Toggleable val="true"/>
    </Frame>

    <Frame type="Frame" name="NavigationTemplate">
        <Frame type="Image" name="Background">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Height val="72"/>
            <Texture val="@UI/Glues_Battlenet_TopNav_Bg"/>
            <TextureType val="Normal"/>
            <Tiled val="false"/>
        </Frame>
        
        <Frame type="Image" name="BackgroundHexShimmer">
            <Anchor side="Top" relative="$parent/Background" pos="Min" offset="4"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Anchor side="Bottom" relative="$parent/Background" pos="Max" offset="-4"/>
            <LayerCount val="2"/>
            
            <Texture val="@UI/Glues_Battlenet_TopNav_HexPattern" layer="0"/>
            <TextureType val="Normal" layer="0"/>
            <Tiled val="true" layer="0"/>
            <AlphaLayer val="true" layer="0"/>
            
            <Texture val="@UI/Glues_Battlenet_TopNav_HexShimmer" layer="1"/>
            <TextureCoords top="0" left="0" bottom="1" right="1" layer="1"/>
            <Alpha val="175" layer="1"/>
            
            <Animation name="Shimmer">
                <Event event="OnShown" action="DirectionForward" frame="$parent"/>
                <Event event="OnShown" action="Play" frame="$parent"/>
            
                <Controller type="LayerUV" layer="1" side="Right" relative="false" sidelock="true" end="Loop" >
                    <Key type="Curve" time="0" value="1" out="linear"/>
                    <Key type="Curve" time="8" value="0" in="linear"/>
                </Controller>
                
                <Controller type="Fade" end="Loop">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="1" value="100"/>
                    <Key type="Curve" time="3" value="200"/>
                    <Key type="Curve" time="4" value="255"/>
                    <Key type="Curve" time="6" value="200"/>
                    <Key type="Curve" time="8" value="0"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="TopLineHighlightAnim">
            <Anchor side="Top" relative="$parent/Background" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Texture val="@UI/Glues_Battlenet_TopNav_TopHighlightAnim"/>
            
            <Animation name="Highlight">
                <Event event="OnShown" action="DirectionForward" frame="$parent"/>
                <Event event="OnShown" action="Play" frame="$parent"/>
                
                <Controller type="Fade" end="Loop">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="1" value="255"/>
                    <Key type="Curve" time="7" value="255"/>
                    <Key type="Curve" time="8" value="0"/>
                </Controller>
                
                <Controller type="Anchor" end="Loop" side="Left" relative="true">
                    <Key type="Curve" time="0" value="0" out="linear"/>
                    <Key type="Curve" time="8" value="1920" out="linear"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="BottomLineHighlightAnim">
            <Anchor side="Bottom" relative="$parent/Background" pos="Max" offset="2"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Texture val="@UI/Glues_Battlenet_TopNav_BottomHighlightAnim"/>
            
            <Animation name="Highlight">
                <Event event="OnShown" action="DirectionForward" frame="$parent"/>
                <Event event="OnShown" action="Play" frame="$parent"/>
                
                <Controller type="Fade" end="Loop">
                    <Key type="Curve" time="0" value="0"/>
                    <Key type="Curve" time="2" value="0"/>
                    <Key type="Curve" time="4.5" value="255"/>
                    <Key type="Curve" time="6" value="255"/>
                    <Key type="Curve" time="8" value="0"/>
                </Controller>
                
                <Controller type="Anchor" end="Loop" side="Left" relative="true">
                    <Key type="Curve" time="0" value="0" out="linear"/>
                    <Key type="Curve" time="2" value="0" out="linear"/>
                    <Key type="Curve" time="8" value="1920" out="linear"/>
                </Controller>
            </Animation>
        </Frame>
        
        <Frame type="Image" name="ButtonDivider1">
            <Anchor side="Top" relative="$parent" pos="Min" offset="4"/>
            <Anchor side="Right" relative="$parent/HomeButton" pos="Max" offset="1"/>
            <Texture val="@UI/Glues_Battlenet_TopNav_ButtonDivider"/>
        </Frame>
        
        <Frame type="Image" name="ButtonDivider2">
            <Anchor side="Top" relative="$parent" pos="Min" offset="4"/>
            <Anchor side="Left" relative="$parent/ClassicButton/HitTestFrame" pos="Max" offset="#StandardNavigation_Mainnav_Divider_Spacing_Horizontal"/>
            <Texture val="@UI/Glues_Battlenet_TopNav_ButtonDivider"/>
        </Frame>
        
        <Frame type="Image" name="ButtonDivider3">
            <Anchor side="Top" relative="$parent" pos="Min" offset="4"/>
            <Anchor side="Left" relative="$parent/BrutalPlusButton/HitTestFrame" pos="Max" offset="#StandardNavigation_Mainnav_Divider_Spacing_Horizontal"/>
            <Texture val="@UI/Glues_Battlenet_TopNav_ButtonDivider"/>
        </Frame>
        
        <Frame type="Image" name="ButtonDivider4">
            <Anchor side="Top" relative="$parent" pos="Min" offset="4"/>
            <Anchor side="Left" relative="$parent/MutationButton/HitTestFrame" pos="Max" offset="#StandardNavigation_Mainnav_Divider_Spacing_Horizontal"/>
            <Texture val="@UI/Glues_Battlenet_TopNav_ButtonDivider"/>
        </Frame>
        
        <Frame type="Image" name="ButtonDivider6">
            <Anchor side="Top" relative="$parent" pos="Min" offset="4"/>
            <Anchor side="Left" relative="$parent/CustomButton/HitTestFrame" pos="Max" offset="#StandardNavigation_Mainnav_Divider_Spacing_Horizontal"/>
            <Texture val="@UI/Glues_Battlenet_TopNav_ButtonDivider"/>
        </Frame>

        <Frame type="Button" name="HomeButton">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <TooltipAnchorPosition val="BottomCenter"/>
            <DisplayType val="ImageLayer"/>
            <ClickSound val="@UI_ActionButtonSelect"/>
            <Width val="80"/>
            <Height val="80"/>
            
            <Frame type="Frame" name="HitTestFrame">
                <Anchor side="Top" relative="$parent" pos="Min" offset="2"/>
                <Anchor side="Bottom" relative="$parent" pos="Max" offset="-8"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
                <Anchor side="Right" relative="$parent" pos="Max" offset="-42"/>
            </Frame>
            
            <Frame type="Image" name="HomeIcon">
                <Anchor side="Top" relative="$parent" pos="Mid" offset="-2"/>
                <Anchor side="Bottom" relative="$parent" pos="Mid" offset="-2"/>
                <Anchor side="Left" relative="$parent" pos="Min" offset="10"/>
                <Width val="50"/>
                <Height val="49"/>
                <Texture val="@UI/Glues_Battlenet_TopNav_Home"/>
                
                <Animation name="Toggled">
                    <Event event="Toggled" action="Reset" frame="$parent"/>
                    <Event event="Toggled" action="Play" frame="$parent"/>
                    <Event event="Normal" action="Reset" frame="$parent"/>
                    <Event event="Disabled" action="Reset" frame="$parent"/>
                    
                    <Controller type="Texture" end="Pause">
                        <Key type="Image" time="0" image="@UI/Glues_Battlenet_TopNav_HomeSelected"/>
                    </Controller>
                </Animation>
            </Frame>
            
            <Frame type="Image" name="HomeIconHover">
                <Anchor relative="$parent/HomeIcon"/>
                <Texture val="@UI/Glues_Battlenet_TopNav_HomeHover"/>
                <Alpha val="0"/>
                
                <Animation name="HomeHover">
                    <Event event="OnMouseEnter" action="Reset, Play" frame="$parent"/>
                    <Event event="OnMouseExit" action="DirectionReverse, Play" frame="$parent"/>
                    
                    <Controller type="Fade" end="Pause">
                        <Key type="Curve" time="0" value="0" out="Fast"/>
                        <Key type="Curve" time=".4" value="255" in="Fast"/>
                    </Controller>
                </Animation>
            </Frame>
        </Frame>

        <Frame type="Button" name="ClassicButton" template="CoopPlusSelectionLayout/NavigationButtonTemplate">
            <Text val="@CoopPlus/Launcher/NavigationClassic"/>
            
            <Frame type="Frame" name="HitTestFrame">
                <Anchor side="Top" relative="$parent/$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/$parent/HomeButton" pos="Max" offset="0"/>
            </Frame>
        </Frame>

        <Frame type="Button" name="BrutalPlusButton" template="CoopPlusSelectionLayout/NavigationButtonTemplate">
            <Text val="@CoopPlus/Launcher/NavigationBrutalPlus"/>
            
            <Frame type="Frame" name="HitTestFrame">
                <Anchor side="Top" relative="$parent/$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/$parent/ClassicButton/HitTestFrame" pos="Max" offset="0"/>
            </Frame>
        </Frame>

        <Frame type="Button" name="MutationButton" template="CoopPlusSelectionLayout/NavigationButtonTemplate">
            <Text val="@CoopPlus/Launcher/NavigationMutation"/>
            
            <Frame type="Frame" name="HitTestFrame">
                <Anchor side="Top" relative="$parent/$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/$parent/BrutalPlusButton/HitTestFrame" pos="Max" offset="0"/>
            </Frame>
        </Frame>

        <Frame type="Button" name="CustomButton" template="CoopPlusSelectionLayout/NavigationButtonTemplate">
            <Text val="@CoopPlus/Launcher/NavigationCustom"/>
            
            <Frame type="Frame" name="HitTestFrame">
                <Anchor side="Top" relative="$parent/$parent" pos="Min" offset="0"/>
                <Anchor side="Left" relative="$parent/$parent/MutationButton/HitTestFrame" pos="Max" offset="0"/>
            </Frame>
        </Frame>

        <Frame type="Frame" name="NavigationBlockFrame">
            <Anchor relative="$parent/Background"/>
            <AcceptsMouse val="true"/>
            <Visible val="false"/>
        </Frame>
    </Frame>

    <Frame type="Frame" name="SubnavTemplate">
        <Anchor side="Top" relative="$Anchor_SubNav" pos="Max" offset="0"/>
        <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
        <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
        <Height val="60"/>
            
        <Frame type="BlurFrame" name="SubnavBackgroundBlur">
            <Anchor relative="$parent/SubnavBackgroundImage"/>
            <BlurAmount val=".3"/>

            <Frame type="Image" name="LowImage">
                <Anchor relative="$parent"/>
                <Color val="0,0,0"/>
                <Alpha val="150"/>
            </Frame>
        </Frame>

        <Frame type="Image" name="SubnavBackgroundImage">
            <Anchor side="Top" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Left" relative="$parent" pos="Min" offset="0"/>
            <Anchor side="Right" relative="$parent" pos="Max" offset="0"/>
            <Height val="52"/>
            <Texture val="@UI/Glues_Battlenet_SubNav_Bg"/>
            <TextureType val="Nineslice"/>
            <Tiled val="true"/>
            <Alpha val="0"/>
            
            <Animation name="FadeNav">
                <Event event="OnShown" action="Reset"/>
